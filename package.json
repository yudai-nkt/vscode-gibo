{
	"name": "vscode-gibo",
	"displayName": "vscode-gibo",
	"description": "gibo interface for Visual Studio Code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
  },
  "homepage": "https://github.com/yudai-nkt/vscode-gibo/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/yudai-nkt/vscode-gibo.git"
  },
  "bugs": "https://github.com/yudai-nkt/vscode-gibo/issues",
	"categories": [
		"Other"
  ],
  "keywords": ["gibo", "git", "gitignore", "github"],
  "license": "SEE LICENSE IN <filename>",
	"activationEvents": [
    "onCommand:vscode-gibo.append-to-gitignore"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
      {
        "command": "vscode-gibo.append-to-gitignore",
        "title": "Create or append to .gitignore",
        "category": "gibo"
      }
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"typescript": "^3.3.1",
		"tslint": "^5.12.1",
		"vscode-test": "^1.2.0"
	}
}
